{
  "name": "all-unpacker",
  "version": "0.0.7",
  "description": "All unpacker for zip, zipx, rar, 7z, tar, gzip, bzip2, lzma,... See theunarchiver.com for complete list, wrapper for unar and lsar.",
  "keywords": [
    "Unarchiver",
    "Unpacker",
    "Zip",
    "Zipx",
    "RAR",
    "7z",
    "Tar",
    "Gzip",
    "Bzip2",
    "LZMA,XZ",
    "Mac OS X",
    "Windows",
    "Linux"
  ],
  "author": {
    "name": "Marc Kronberg",
    "email": "krocon@gmail.com",
    "url": "www.krocon.de"
  },
  "contributors": [
    "l. stubbs <technoexpressnet@gmail.com>"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha --dir .tmp/coverage",
    "coveralls": "npm install coveralls && cat .tmp/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "install": "node installer.js"
  },
  "files": [
    "*.js",
    "*.md",
    "test",
    "!demoFirstEntry.js"
  ],
  "dependencies": {
    "array-map": "~0.0.0",
    "npmlog": "^1.2.1"
  },
  "optionalDependencies": {
    "system-installer": "^1.0.6",
    "node-wget": "^0.4.2",
    "unzipper": "^0.8.11"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krocon/node-unpack-all/issues"
  },
  "homepage": "https://github.com/krocon/node-unpack-all.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/techno-express/node-unpack-all"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "fs-extra": "^5.0.0",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "sinon": "^4.3.0"
  }
}
